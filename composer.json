{
	"name": "codekandis/toolkit",
	"description": "`codekandis/toolkit` is a library providing interfaces and classes for several purposes.",
	"keywords": [
		"php",
		"library",
		"toolkit",
		"interfaces",
		"classes"
	],
	"minimum-stability": "stable",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Ramelow",
			"email": "info@codekandis.net",
			"homepage": "https://codekandis.net",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@codekandis.net",
		"issues": "https://github.com/codekandis/toolkit/issues",
		"docs": "https://github.com/codekandis/toolkit/blob/master/README.md",
		"source": "https://github.com/codekandis/toolkit/tree/master"
	},
	"require": {
		"php": ">=8.4",
		"codekandis/types": "~1.0"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"codekandis/phpunit": "~5.0",
		"rector/rector": "~1.2"
	},
	"autoload": {
		"psr-4": {
			"CodeKandis\\ToolKit\\": [
				"src/"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CodeKandis\\ToolKit\\Tests\\": [
				"tests/"
			]
		}
	},
	"scripts": {
		"test": [
			"Composer\\Config::disableProcessTimeout",
			"/usr/bin/env php ./vendor/phpunit/phpunit/phpunit --configuration ./build/phpunit.xml -dxdebug.mode=off --no-coverage"
		],
		"test-coverage": [
			"Composer\\Config::disableProcessTimeout",
			"/usr/bin/env php ./vendor/phpunit/phpunit/phpunit --configuration ./build/phpunit.xml -dxdebug.mode=coverage"
		]
	}
}
